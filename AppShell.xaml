<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Quezada_Evaluacion3P.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Quezada_Evaluacion3P"
    xmlns:v="clr-namespace:Quezada_Evaluacion3P.Views"
    Title="Quezada_Evaluacion3P">

    <TabBar>
        <Tab Title="Games" Icon="mainpage.png">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
        <Tab Title="Saved Games" Icon="iconinfo.png">
            <ShellContent ContentTemplate="{DataTemplate local:AQGamesGuardados}" />
        </Tab>
    </TabBar>
</Shell>
